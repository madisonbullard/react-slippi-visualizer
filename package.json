{
  "name": "react-slippi-visualizer",
  "version": "0.1.0",
  "description": "A React component for displaying SSBM replays in the browser based on Project Slippi",
  "author": "Madison Bullard <madison.bullard@gmail.com>",
  "license": "(ISC OR GPL-3.0)",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --env.prod",
    "start": "webpack-dev-server --inline --hot --open --env.dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "babel-plugin-syntax-dynamic-import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madisonbullard/react-slippi-visualizer.git"
  },
  "bugs": {
    "url": "https://github.com/madisonbullard/react-slippi-visualizer/issues"
  },
  "homepage": "https://github.com/madisonbullard/react-slippi-visualizer#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "9.x",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-css-modules": "^5.2.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "5.x",
    "eslint-config-react-app": "^4.0.0",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "postcss": {
    "parser": "sugarss",
    "map": false,
    "plugins": {
      "postcss-import": {},
      "postcss-preset-env": {},
      "cssnano": {}
    }
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
